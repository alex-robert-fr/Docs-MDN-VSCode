{
	"name": "docs-mdn",
	"displayName": "Docs MDN",
	"description": "Search for the selected word on the mdn",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.59.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:docs-mdn.docsmdnglobal",
		"onCommand:docs-mdn.docsmdnprecise"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "docs-mdn.docsmdnglobal",
				"title": "Search the MDN global docs"
			},
			{
				"command": "docs-mdn.docsmdnprecise",
				"title": "Search precisely in the MDN docs"
			}
		],
		"menus": {
			"editor/context": [
				{
					"when": "editorHasSelection",
					"command": "docs-mdn.docsmdnglobal",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.59.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	}
}
